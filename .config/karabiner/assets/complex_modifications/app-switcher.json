{
  "title": "app switcher",
  "rules": [
    {
      "description": "Hyper Application",
      "manipulators": [
        {
          "from": {
            "key_code": "comma",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome'"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "period",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Visual Studio Code'"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "slash",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'iterm'"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
