{
    "title": "chrome (vimium) ctrl-u and ctrl-k as readline",
    "rules": [
        {
            "description": "chrome (vimium) ctrl-u and ctrl-k as readline",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "bundle_identifiers": [
                                "^com.google.Chrome$"
                            ],
                            "type": "frontmost_application_if"
                        }
                    ],
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "mandatory": [
                                "left_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "home",
                            "modifiers": [
                                "shift"
                            ]
                        },
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ],
                    "type": "basic"
                },
                {
                    "conditions": [
                        {
                            "bundle_identifiers": [
                                "^com.google.Chrome$"
                            ],
                            "type": "frontmost_application_if"
                        }
                    ],
                    "from": {
                        "key_code": "k",
                        "modifiers": {
                            "mandatory": [
                                "left_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "end",
                            "modifiers": [
                                "shift"
                            ]
                        },
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
