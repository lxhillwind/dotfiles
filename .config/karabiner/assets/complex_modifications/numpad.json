{
  "title": "<https://ke-complex-modifications.pqrs.org/#numpad> (modified: Numlock always off)",
  "rules": [
    {
      "description": "Numpad Keys as non-number key",
      "manipulators": [
        {
          "from": {
            "key_code": "keypad_7",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {
              "key_code": "home"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_8",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_9",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_4",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_6",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_1",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {
              "key_code": "end"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_2",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_3",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_period",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_0",
            "modifiers": { "optional": ["any"] }
          },
          "to": [
            {
              "key_code": "application"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "keypad_5"
          },
          "to": [
            {
              "key_code": "vk_none"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
